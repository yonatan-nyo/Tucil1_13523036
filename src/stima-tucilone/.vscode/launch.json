  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "App",
      "request": "launch",
      "mainClass": "tucilone.stime/tucilone.stime.App",
      "projectName": "stima-tucilone"
    },
    {
      "type": "java",
      "name": "App",
      "request": "launch",
      "mainClass": "chisli.App"
    },
    {
      "type": "java",
      "name": "Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "App",
      "request": "launch",
      "mainClass": "chisli/chisli.App",
      "projectName": "chisli"
    },
    {
      "type": "java",
      "name": "Launch JavaFX App",
      "request": "launch",
      "mainClass": "chisli.App",
      "vmArgs": "--module-path ${workspaceFolder}/libs/chislib.jar --add-modules javafx.controls,javafx.fxml",
      "projectName": "chisli"
    }
  ]
}
